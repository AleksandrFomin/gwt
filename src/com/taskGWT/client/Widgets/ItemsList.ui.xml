<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel>
        <g:HorizontalPanel ui:field="horizontalPanel">
            <g:VerticalPanel ui:field="listPanel" addStyleNames="mainPanel">
                <g:HorizontalPanel ui:field="btnPanel">
                    <g:Button ui:field="addBtn" addStyleNames="buttons">ADD</g:Button>
                    <g:Button ui:field="deleteBtn" addStyleNames="buttons">DELETE</g:Button>
                </g:HorizontalPanel>
            </g:VerticalPanel>
        </g:HorizontalPanel>
    </g:HTMLPanel>
</ui:UiBinder>