<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel width="100%">
        <g:HorizontalPanel width="100%" ui:field="horizontalPanel">
            <g:VerticalPanel width="100%" ui:field="listPanel">
                <g:HorizontalPanel width="100%">
                    <g:Button ui:field="deleteBtn">DELETE</g:Button>
                    <g:Button ui:field="addBtn">ADD</g:Button>
                </g:HorizontalPanel>
            </g:VerticalPanel>
        </g:HorizontalPanel>
    </g:HTMLPanel>
</ui:UiBinder>